int my_strcasecmp(const char *s1, const char *s2){
  int dist = ('a'-'A');

  putchar('c');
  
  while(*s1 == *s2 || (*s1+dist) == *s2 || (*s1-dist) == *s2){
    if(*s1 == '\0'){
      return 0;
    } 
    s1++;
    s2++;
  }

  return (*s1-*s2);


  /*while(*s1 != '\0' && *s2 != '\0'){
