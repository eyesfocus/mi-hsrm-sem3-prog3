#include <stdio.h>
#include <stdlib.h>

typedef struct _node{
  struct _node *next;
  char[30] data;
} knoten;

typedef node *nodeptr;
int size = 0;


nodeptr add(nodeptr list, int data){
  nodeptr new_node = malloc(sizeof(struct _node));
  new_node->data = data;

  if(is_empty(list)){
    list = new_node;
  } else{
    nodeptr temp = list;
    while(temp->next !=NULL){
      temp = temp->next;
    }
  }
  temp->next = new_node;
  size++;

  return list;
}

nodeptr del(nodeptr list, int no){


}


int is_empty(nodeptr list){
  return (list==NULL);
}

int main(int argc, char *argv[]){
  int silben = argv[1];
  int i;
  int sizeOfList=0;
  node head;
  
  for (i = 2; i < argc; i++){
     akt = add(akt, argv[i]);
     sizeOfList++;
  }

  while(sizeOfList){

    printf("%s", head->next->data);

  }


      

}
